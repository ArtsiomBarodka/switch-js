{
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "outDir": "dist",
    "declaration": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "baseUrl": "./",
    "paths": {
      "mytypings/*": ["custom-typings/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
